                Case Else
                    Call File_Error(sts, com, "ç›å…ÉfÅ[É^")
                    Exit Function
            End Select

            Select Case StrConv(ZAIKOREC.GOODS_ON, vbUnicode)
                Case "0"
                    
                    Not_GOODS = False
                    
                                 
                    For i = 0 To UBound(SHO_SOKO)
                    
                    
                        If StrConv(ZAIKOREC.Soko_No, vbUnicode) = SHO_SOKO(i) Then
                            Not_GOODS = True
