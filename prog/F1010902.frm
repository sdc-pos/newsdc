Private Sub Option1_Click(Index As Integer)

Dim com     As Integer
Dim sts     As Integer

Dim Edit    As String

    If Len(Trim(Text(ptxPacking_No).Text)) = 0 Then
        Beep
        MsgBox "å¬ëïî†áÇÇëIëÇµÇƒâ∫Ç≥Ç¢ÅB"
        Text(ptxPacking_No).SetFocus
        Exit Sub
    End If

    Call UniCode_Conv(K1_TPACKING.PACKING_NO, Text(ptxPacking_No).Text)
    Call UniCode_Conv(K1_TPACKING.RANK, StrConv(Option1(Index).Caption, vbNarrow))

    Call UniCode_Conv(K1_TPACKING.Soko_No, "")
    Call UniCode_Conv(K1_TPACKING.Retu, "")
    Call UniCode_Conv(K1_TPACKING.Ren, "")

    com = BtOpGetGreaterEqual

    List1.Clear

    Do
        sts = BTRV(com, TPACKING_POS, TPACKINGREC, Len(TPACKINGREC), K1_TPACKING, Len(K1_TPACKING), 1)
        Select Case sts
            Case BtNoErr
            Case BtErrEOF
                Exit Do
            Case Else
                Call File_Error(sts, com, "íIï å¬ëïî†É}ÉXÉ^")
                F1010902.Hide
        End Select
    
        If Trim(StrConv(TPACKINGREC.PACKING_NO, vbUnicode)) <> Trim(Text(ptxPacking_No).Text) Or _
            Trim(StrConv(TPACKINGREC.RANK, vbUnicode)) <> Trim(StrConv(Option1(Index).Caption, vbNarrow)) Then
            
            Exit Do
        
        End If
    
        Edit = StrConv(TPACKINGREC.Soko_No, vbUnicode) & "-" & StrConv(TPACKINGREC.Retu, vbUnicode) & "-" & StrConv(TPACKINGREC.Ren, vbUnicode)
    
        List1.AddItem Edit
    
        com = BtOpGetNext
    
    Loop

End Sub

Private Sub Text_GotFocus(Index As Integer)
    
    If Text(Index).TabStop = True Then
        Text(Index) = Trim(Text(Index).Text)
        Text(Index).SelStart = 0
        Text(Index).SelLength = Len(Text(Index).Text)
    End If


End Sub
