        Case vbKeyDelete
    
            For j = Start_Pos To UBound(MENU_TBL) - 1
                            
            
                MENU_TBL(j).JGYOBU = MENU_TBL(j + 1).JGYOBU
                MENU_TBL(j).NAIGAI = MENU_TBL(j + 1).NAIGAI
                MENU_TBL(j).MENU_NO = MENU_TBL(j + 1).MENU_NO
                MENU_TBL(j).MENU_DSP = MENU_TBL(j + 1).MENU_DSP
            
            
            Next j
        
        
        
        
            MENU_TBL(UBound(MENU_TBL)).JGYOBU = ""
            MENU_TBL(UBound(MENU_TBL)).NAIGAI = ""
            MENU_TBL(UBound(MENU_TBL)).MENU_NO = ""
            MENU_TBL(UBound(MENU_TBL)).MENU_DSP = ""
        
        
        
            Call GAMEN_TBL_SET_PROC
    
    End Select


End Sub
