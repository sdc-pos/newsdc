Private Function TBL_Set_Proc() As Integer
'----------------------------------------------------------------------------
'                   “∆≠∞èÓïÒÇÃÉeÅ[ÉuÉãÉZÉbÉg
'----------------------------------------------------------------------------
Dim i       As Integer
Dim sts     As Integer

    TBL_Set_Proc = True

    For i = 0 To UBound(MENU_TBL)
    
    
        MENU_TBL(i).YOIN = StrConv(P_MENUREC.SAGYO(i).YOIN, vbUnicode)
        
        
        
        
        If MENU_TBL(i).YOIN = END_YOIN Then
        
            MENU_TBL(i).YOIN_DNAME = END_DNAME
            Call UniCode_Conv(YOINREC.PARAM_F, "0")
        Else
            
            Call UniCode_Conv(K0_YOIN.CODE_TYPE, Mid(StrConv(P_MENUREC.SAGYO(i).YOIN, vbUnicode), 1, 1))
            Call UniCode_Conv(K0_YOIN.YOIN_CODE, Mid(StrConv(P_MENUREC.SAGYO(i).YOIN, vbUnicode), 2, 1))
            sts = BTRV(BtOpGetEqual, YOIN_POS, YOINREC, Len(YOINREC), K0_YOIN, Len(K0_YOIN), 0)
            Select Case sts
                Case BtNoErr
                    MENU_TBL(i).YOIN_DNAME = StrConv(YOINREC.YOIN_DNAME, vbUnicode)
                Case BtErrKeyNotFound
                    Call UniCode_Conv(YOINREC.PARAM_F, "0")
                Case Else
                    Call File_Error(sts, BtOpGetEqual, "óvàˆÉ}ÉXÉ^")
                    Exit Function
        
            End Select
        End If
        
        MENU_TBL(i).PARAM_F = StrConv(YOINREC.PARAM_F, vbUnicode)
        MENU_TBL(i).PARAM = StrConv(P_MENUREC.SAGYO(i).PARAM, vbUnicode)
        
        
        If MENU_TBL(i).PARAM_F = "1" Then
        
            Call UniCode_Conv(K0_MTS.MUKE_CODE, Mid(StrConv(P_MENUREC.SAGYO(i).PARAM, vbUnicode), 1, 8))
            Call UniCode_Conv(K0_MTS.SS_CODE, Mid(StrConv(P_MENUREC.SAGYO(i).PARAM, vbUnicode), 9, 8))
            sts = BTRV(BtOpGetEqual, MTS_POS, MTSREC, Len(MTSREC), K0_MTS, Len(K0_MTS), 0)
            Select Case sts
                Case BtNoErr
                    MENU_TBL(i).MUKE_DNAME = StrConv(MTSREC.MUKE_DNAME, vbUnicode)
                Case BtErrKeyNotFound
                Case Else
                    Call File_Error(sts, BtOpGetEqual, "å¸ÇØêÊä«óùÉ}ÉXÉ^")
                    Exit Function
        
            End Select
        
        
        
        End If
        
        MENU_TBL(i).Disp = StrConv(P_MENUREC.SAGYO(i).Disp, vbUnicode)
        MENU_TBL(i).LOG_OUT = "1"
    
    
    
    
    Next i
    
    TBL_Set_Proc = False

End Function

