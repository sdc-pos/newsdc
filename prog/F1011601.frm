    hStatusWnd = CreateStatusWindow(WS_VISIBLE Or WS_CHILD Or CCS_BOTTOM Or SBARS_SIZEGRIP, _
        "ÉnÉìÉfÉBÉÅÉjÉÖÅ[ìoò^âÊñ Å@", Me.hwnd, 0)

