        MENU_TBL(i).YOIN_DNAME = ""
        MENU_TBL(i).PARAM = ""
        MENU_TBL(i).MUKE_DNAME = ""
        MENU_TBL(i).Disp = ""
        MENU_TBL(i).LOG_OUT = ""
    Next i





       
