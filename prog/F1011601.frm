Private Sub Text7_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
        
Dim j   As Integer
        
        
        
    Select Case KeyCode
        
        Case vbKeyInsert
            
            For j = UBound(MENU_TBL) - 1 To Index Step -1
                            
            
                MENU_TBL(j + 1).YOIN = MENU_TBL(j).YOIN
                MENU_TBL(j + 1).YOIN_DNAME = MENU_TBL(j).YOIN_DNAME
                MENU_TBL(j + 1).Disp = MENU_TBL(j).Disp
                MENU_TBL(j + 1).PARAM = MENU_TBL(j).PARAM
                MENU_TBL(j + 1).PARAM_F = MENU_TBL(j).PARAM_F
                MENU_TBL(j + 1).MUKE_DNAME = MENU_TBL(j).MUKE_DNAME
            
            
            Next j
        
        
        
        
            MENU_TBL(Index).YOIN = ""
            MENU_TBL(Index).YOIN_DNAME = ""
            MENU_TBL(Index).Disp = ""
            MENU_TBL(Index).PARAM = ""
            MENU_TBL(Index).PARAM_F = ""
            MENU_TBL(Index).MUKE_DNAME = ""
        
        
        
            Call GAMEN_TBL_SET_PROC
        
        
        Case vbKeyDelete
    
            For j = Index To UBound(MENU_TBL) - 1
                            
                MENU_TBL(j).YOIN = MENU_TBL(j + 1).YOIN
                MENU_TBL(j).YOIN_DNAME = MENU_TBL(j + 1).YOIN_DNAME
                MENU_TBL(j).Disp = MENU_TBL(j + 1).Disp
                MENU_TBL(j).PARAM = MENU_TBL(j + 1).PARAM
                MENU_TBL(j).PARAM_F = MENU_TBL(j + 1).PARAM_F
                MENU_TBL(j).MUKE_DNAME = MENU_TBL(j + 1).MUKE_DNAME
                            
            
            
            Next j
        
        
            MENU_TBL(UBound(MENU_TBL)).YOIN = ""
            MENU_TBL(UBound(MENU_TBL)).YOIN_DNAME = ""
            MENU_TBL(UBound(MENU_TBL)).Disp = ""
            MENU_TBL(UBound(MENU_TBL)).PARAM = ""
            MENU_TBL(UBound(MENU_TBL)).PARAM_F = ""
            MENU_TBL(UBound(MENU_TBL)).MUKE_DNAME = ""
        
        
        
            Call GAMEN_TBL_SET_PROC


    End Select
End Sub
