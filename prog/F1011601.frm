Public Function MENU_Code_Set_Proc() As Integer
'----------------------------------------------------------------------------
'                   ÉÅÉjÉÖÅ[ÇÃÉZÉbÉg
'----------------------------------------------------------------------------

Dim com     As Integer
Dim sts     As Integer


    MENU_Code_Set_Proc = True

    Combo1(pcmb1MENU_DSP).Clear




    Call UniCode_Conv(K0_P_MENU.JGYOBU, Right(Combo1(pcmb1JGYOBU), 1))
    Call UniCode_Conv(K0_P_MENU.NAIGAI, "1")
    Call UniCode_Conv(K0_P_MENU.MENU_NO, "")


    com = BtOpGetGreater

    Do
        
        DoEvents

        sts = BTRV(com, P_MENU_POS, P_MENUREC, Len(P_MENUREC), K0_P_MENU, Len(K0_P_MENU), 0)

        Select Case sts
            Case BtNoErr
            
                If StrConv(P_MENUREC.JGYOBU, vbUnicode) <> Right(Combo1(pcmb1JGYOBU), 1) Or _
                   StrConv(P_MENUREC.NAIGAI, vbUnicode) <> "1" Then
                   Exit Do
                End If
            
            Case BtErrEOF
                Exit Do
            Case Else
                Call File_Error(sts, com, "“∆≠∞ä«óùœΩ¿")
                Exit Function

        End Select
    
        Combo1(pcmb1MENU_DSP).AddItem StrConv(P_MENUREC.MENU_DSP, vbUnicode) & Space(10) & StrConv(P_MENUREC.MENU_NO, vbUnicode)



        com = BtOpGetNext

    Loop




    MENU_Code_Set_Proc = False

End Function
Private Function List_Disp_Proc() As Integer
