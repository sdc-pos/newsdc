            End If
        
            Write #FileNo, Format(CLng(StrConv(SUMZREC.PPSC_ZAI_QTY, vbUnicode)), "#0"),
            Write #FileNo, Format(CLng(StrConv(SUMZREC.BU_ZAI_QTY, vbUnicode)), "#0")
        
        
        
        End If
    
        com = BtOpGetNext
    
    Loop

    Close #FileNo
    Call Input_UnLock             'âÊñ çÄñ⁄ÉçÉbÉNâèú
    
    
    If Not Auto_Mode Then
        Beep
        MsgBox "Åu" & FileName & "ÅvÇÕê≥èÌÇ…èoóÕÇ≥ÇÍÇ‹ÇµÇΩÅB"
    End If
    OUTPUT_Proc = False


    Exit Function

Error_Proc:

    If Err.Number = 70 Then
