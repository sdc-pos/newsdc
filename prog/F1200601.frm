Private Function Err_Chk() As Integer
'----------------------------------------------------------------------------
'                   ì¸óÕÉGÉâÅ[É`ÉFÉbÉNèàóù
'----------------------------------------------------------------------------
Dim i   As Integer
    
    Err_Chk = True

    
    If Not IsNumeric(Text(ptxYY).Text) Then
